{extend name="extra@index/common"}

{block name='style'}
<link rel="stylesheet" href="/static/index/css/index.css" />
<style>
    li{
        list-style: none;
    }
    .list_lh {
        height: 150px;
        overflow: hidden;
        margin-top: 10px;
    }
    td{
        text-align: center;
        width:15%;
    }
    .tds:nth-of-type(1) {
        width: 30%;
    }
    .tds:nth-of-type(6){
        width:10%;
    }
    .rczp{
        background-color: #f5f5f5;
        color:#333;
    }
    .rczp td{
        border-right:1px solid #fff;
        vertical-align: center;
        height:40px;
    }
    .list_lh td{
        height:50px;
        vertical-align: center;
    }
    .list_lh li{
        border-bottom:1px dashed #ddd;
    }
</style>
{/block}

{block name='banner'}


