<!--================轮播图==========================-->
<section class="bitBanner" id="banner">
    <div class="swiper-container">
        <div class="swiper-wrapper">
            {foreach $banner as $v}
            <div class="swiper-slide">
                <a href="javascript:;" title="{$v.title}" alt="{$v.alt}" ><img src="/static/{$v.path}" alt="{$v.alt}" data-bgposition="center top" data-bgfit="cover" data-bgrepeat="no-repeat"></a>
            </div>
            {/foreach}
        </div>

        <!-- 如果需要分页器 -->
        <div class="swiper-pagination"></div>
    </div>
</section>
{/block}
